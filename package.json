{
  "name": "sahil-langoo-personal-website",
  "type": "module",
  "version": "1.0.9",
  "private": true,
  "description": "Sahil's personal website built with Astro and Tailwind CSS",
  "author": "Sahil Langoo",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && jampack ./dist --nocache --cleancache",
    "tbuild": "astro build && jampack ./dist --fast --nowrite --cleancache",
    "preview": "astro preview",
    "astro": "astro",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "format": "prettier --plugin-search-dir=. --write .",
    "prepare": "husky install",
    "lint": "eslint --ext .astro,.js,.ts ."
  },
  "devDependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/mdx": "^2.0.4",
    "@astrojs/partytown": "^2.0.3",
    "@astrojs/rss": "^4.0.2",
    "@astrojs/sitemap": "^3.0.4",
    "@astrojs/tailwind": "^5.1.0",
    "@divriots/jampack": "^0.23.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "astro": "^4.1.2",
    "astro-breadcrumbs": "^1.8.1",
    "astro-eslint-parser": "^0.15.0",
    "astro-favicons": "^2.0.2",
    "astro-icon": "^1.0.2",
    "astro-integration-lottie": "^0.2.3",
    "astro-webmanifest": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.30.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "lottie-web": "^5.12.2",
    "mdast-util-to-string": "^4.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.0",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "reading-time": "^1.5.0",
    "rehype-external-links": "^3.0.0",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.1",
    "theme-change": "^2.5.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.13.1",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json}": [
      "prettier --plugin-search-dir=. --write"
    ]
  }
}
