{
  "name": "sahil-langoo-personal-website",
  "type": "module",
  "homepage": "https://sahillangoo.com",
  "version": "1.3.0",
  "private": true,
  "description": "Sahil's personal website built with Astro and Tailwind CSS",
  "author": "Sahil Langoo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sahillangoo/sahillangoo.github.io.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.50.0"
  },
  "scripts": {
    "dev": "astro dev",
    "preview": "astro preview",
    "build": "astro build && jampack ./dist",
    "lint": "eslint --ext .astro,.js,.ts .",
    "format:check": "prettier --check .",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "sync": "astro sync",
    "check": "astro check",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{astro,html,js,jsx,svelte,ts,tsx,vue}": [
      "prettier --write ."
    ]
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@divriots/jampack": "^0.24.4",
    "@tailwindcss/typography": "^0.5.14",
    "astro": "^4.13.3",
    "astro-breadcrumbs": "^2.3.1",
    "astro-favicons": "^2.0.2",
    "astro-icon": "^1.1.0",
    "astro-min": "^1.3.1",
    "astro-webmanifest": "^1.0.0",
    "daisyui": "^4.12.10",
    "mdast-util-to-string": "^4.0.0",
    "reading-time": "^1.5.0",
    "rehype-external-links": "^3.0.0",
    "sharp": "^0.33.4",
    "tailwindcss": "^3.4.9",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "astro-eslint-parser": "^1.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.9",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.1.2+sha512.127dc83b9ea10c32be65d22a8efb4a65fb952e8fefbdfded39bdc3c97efc32d31b48b00420df2c1187ace28c921c902f0cb5a134a4d032b8b5295cbfa2c681e2"
